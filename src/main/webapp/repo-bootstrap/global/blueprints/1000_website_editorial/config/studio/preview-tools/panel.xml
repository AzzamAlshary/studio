<!--
- - - - - - - - - - - - 
Now supports JSON too!
- - - - - - - - - - - - 
{
  "modules": [
    { 
      "id": "craftercms.ice.simulator", 
      "title": "publishingChannel", 
      "config": { 
        "channels": [
          { "width": 375, "title": "smartPhone" }
        ] 
      } 
    }
  ]
}
-->
<panel>
  <modules>
    <module>
      <moduleName>craftercms.ice.search</moduleName>
      <title>searchPanel</title>
    </module>
    <module>
      <moduleName>craftercms.ice.components</moduleName>
      <title>componentsPanel</title>
    </module>
    <module>
      <moduleName>craftercms.ice.assets</moduleName>
      <title>assetsPanel</title>
    </module>
    <module>
      <moduleName>craftercms.ice.audiences</moduleName>
      <title>audiencesPanel</title>
    </module>
    <module>
      <moduleName>craftercms.ice.pageExplorer</moduleName>
      <title>pageExplorerPanel</title>
    </module>
    <module>
      <moduleName>medium-panel</moduleName>
      <title>publishingChannel</title>
      <config>
        <channels>
          <channel>
            <title>desktop</title>
            <value>browser</value>
            <width></width>
            <height></height>
          </channel>
          <channel>
            <title>smartPhone</title>
            <value>iphone</value>
            <width>375</width>
            <height>667</height>
          </channel>
          <channel>
            <title>tablet</title>
            <value>ipad</value>
            <width>768</width>
            <height>1024</height>
          </channel>
        </channels>
      </config>
    </module>
    <module>
      <moduleName>craftercms.siteExplorerPanel</moduleName>
      <title>siteExplorerPanel</title>
    </module>
  </modules>
</panel>
