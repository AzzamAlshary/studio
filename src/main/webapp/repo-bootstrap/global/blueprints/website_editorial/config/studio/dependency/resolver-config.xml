				</mimetype>
			</mimetypes>
		</dependency-type>
		<dependency-type>
			<name>script</name>
			<mimetypes>
				<mimetype>
					<name>application/xml</name>
					<patterns>
						<pattern>/scripts/([^&lt;"]+)\.groovy</pattern>
					</patterns>
				</mimetype>
				<mimetype>
					<name>application/javascript</name>
					<patterns>
