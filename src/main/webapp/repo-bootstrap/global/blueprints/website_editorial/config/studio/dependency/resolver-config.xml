					<patterns>
						<pattern>/scripts/([^&lt;"]+)\.groovy</pattern>
					</patterns>
				</mimetype>
			</mimetypes>
		</dependency-type>
		<dependency-type>
			<name>document</name>
			<mimetypes>
				<mimetype>
					<name>application/xml</name>
					<patterns>
						<pattern>/site/documents/([^&lt;"'\)]+)</pattern>
					</patterns>
				</mimetype>
				<mimetype>
					<name>application/javascript</name>
					<patterns>
						<pattern>/site/documents/([^&lt;"'\)]+)</pattern>
					</patterns>
				</mimetype>
				<mimetype>
					<name>text/css</name>
					<patterns>
						<pattern>/site/documents/([^&lt;"'\)]+)</pattern>
