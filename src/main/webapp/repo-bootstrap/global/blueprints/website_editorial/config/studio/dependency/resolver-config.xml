<?xml version="1.0" encoding="UTF-8"?>
<dependency-resolver>
	<types>
		<dependency-type>
			<name>page</name>
			<mimetypes>
				<mimetype>
					<name>application/xml</name>
					<patterns>
						<pattern>/site/website/([^&lt;]+)\.xml</pattern>
					</patterns>
				</mimetype>
				<mimetype>
					<name>application/javascript</name>
					<patterns>
						<pattern>/site/website/([^&lt;]+)\.xml</pattern>
					</patterns>
				</mimetype>
				<mimetype>
					<name>text/css</name>
					<patterns>
						<pattern>/site/website/([^&lt;]+)\.xml</pattern>
					</patterns>
				</mimetype>
				<mimetype>
					<name>text/x-freemarker</name>
					<patterns>
						<pattern>/site/website/([^&lt;]+)\.xml</pattern>
					</patterns>
				</mimetype>
			</mimetypes>
		</dependency-type>
